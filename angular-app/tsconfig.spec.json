{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "moduleResolution": "node",
    "types": [
      "jasmine", "node"
    ],
    "typeRoots": ["node_modules/@types"]
  },
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}
